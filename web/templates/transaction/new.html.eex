<h4>Transfer your asset</h4>
<%= form_for @conn, transaction_path(@conn, :create), [as: :transaction, multipart: true], fn f -> %>
  <div class="form-group">
    <%= label f, :asset, class: "control-label" %>
    <%= select(f, :role, @assets) %>
  </div>

  <div class="form-group">
    <%= label f, :to, class: "control-label" %>
    <%= text_input f, :to, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= submit "Submit", class: "btn btn-primary" %>
  </div>
<% end %>
